import axios from '../../axios'

// 胸透-获取列表
export const getList = (data) => {
    return axios({
        url: "/laboratory/getList",
        method: "post",
        data
    })
}
// 胸透-批量正常
export const label = (data) => {
  return axios({
      url: "/laboratory/batchCommit",
      method: "post",
      data
  })
}
// 胸透-编辑
export const change = (data) => {
  return axios({
      url: "/laboratory/update",
      method: "post",
      data
  })
}
// 胸透-今日总情况
export const toDay = (data) => {
  return axios({
      url: "/laboratory/toDay",
      method: "post",
      data
  })
}

